{
	"_version": "1.12.0",
	"sap.app": {
		"id": "ui5demo.sapui5app",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "ui5template.basicSAPUI5ApplicationProject",
			"version": "1.40.12"
		},
		"dataSources": {
			"Northwind": {
				"uri": "/destinations/Northwind/V2/Northwind/Northwind.svc/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0"
				}
			}
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},
	"sap.ui5": {
		"flexEnabled": false,
		"rootView": {
			"viewName": "ui5demo.sapui5app.view.App",
			"type": "XML",
			"async": true,
			"id": "App"
		},
		"dependencies": {
			"minUI5Version": "1.65.6",
			"libs": {
				"sap.ui.layout": {},
				"sap.ui.core": {},
				"sap.m": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "ui5demo.sapui5app.i18n.i18n"
				}
			},
			"appData": {
				"type": "sap.ui.model.json.JSONModel",
				"uri": "model/appData.json"
			},
			"Model": {
				"dataSource": "Northwind",
				"settings": {
					"synchronizationMode": "None"
				}
			}
		},
		"resources": {
			"css": [{
				"uri": "css/style.css"
			}]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"async": true,
				"viewPath": "ui5demo.sapui5app.view",
				"controlAggregation": "pages",
				"controlId": "app",
				"clearControlAggregation": false
			},
			"routes": [{
				"name": "home",
				"pattern": "",
				"target": [
					"Home"
				]
			}, {
				"name": "about",
				"pattern": "about",
				"target": [
					"About"
				]
			}, {
				"name": "users",
				"pattern": "users",
				"target": [
					"Users"
				]
			}, {
				"name": "employee",
				"pattern": "employee",
				"target": [
					"Employee"
				]
			}],
			"targets": {
				"Home": {
					"viewType": "XML",
					"transition": "slide",
					"clearControlAggregation": false,
					"viewId": "home",
					"viewName": "Home"
				},
				"App": {
					"viewType": "XML",
					"viewName": "App"
				},
				"About": {
					"viewType": "XML",
					"viewName": "About"
				},
				"Employee": {
					"viewType": "XML",
					"viewName": "Employee"
				},
				"Users": {
					"viewType": "XML",
					"viewName": "Users"
				}
			}
		}
	}
}
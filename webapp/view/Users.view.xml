<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" controllerName="ui5demo.sapui5app.controller.Users"
	xmlns:html="http://www.w3.org/1999/xhtml">
	<App>
		<pages id="users">
			<Page title="Users">
				<content>
					<Button text="Go Back" press="back"> </Button>
					<List noDataText="Drop list items here" id="idlist" items="{/users}" headerText="Users List">
						<items>
							<StandardListItem type="Navigation" title="{name}" description="Name is: {email}" icon="sap-icon://picture" id="item0"/>
						</items>
					</List>
				</content>
			</Page>
		</pages>
	</App>
</mvc:View>